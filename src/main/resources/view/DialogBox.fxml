<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="400" maxWidth="400" prefWidth="400.0" style="-fx-background-color: #e1eff7;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" minHeight="100" prefHeight="200" maxHeight="Infinity" text="Label" wrapText="true">
         <font>
            <Font name="Corbel" size="15.0" />
         </font>
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin></Label>
        <ImageView fx:id="displayPicture" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
