<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" minHeight="-Infinity" style="-fx-background-color: #545F66; -fx-background-radius: 5;" text="Label" textFill="#d0f4ea" wrapText="true">
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding></Label>
      <AnchorPane prefHeight="16.0" prefWidth="25.0">
         <children>
          <ImageView fx:id="displayPicture" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true" />
            <Circle fx:id="myCircle" fill="DODGERBLUE" layoutX="17.0" layoutY="17.0" radius="17.5" stroke="BLACK" strokeType="INSIDE" />
         </children>
      </AnchorPane>
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
  </padding>
</fx:root>
