<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" spacing="5.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" style="-fx-background-radius: 10; -fx-background-color: rgb(0,34,99);" text="Label" textFill="#f8f2bf" wrapText="true">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
         <effect>
            <DropShadow color="#00000081" offsetY="3.0" />
         </effect></Label>
      <VBox fx:id="identity" alignment="TOP_CENTER">
         <children>
              <ImageView fx:id="displayPicture" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="imageLabel" text="Label" textFill="#00113a">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="12.0" right="12.0" top="15.0" />
    </padding>
</fx:root>
