<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox style="-fx-border-width: 2; -fx-border-radius: 5; -fx-border-color: #ffb469#ffb469; -fx-padding: 10; -fx-border-style: solid inside; -fx-background-color: #ffefd6#ffefd6;">
         <children>
            <Label fx:id="dialog" text="speech" wrapText="true">
               <font>
                  <Font name="Monospaced Regular" size="14.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Label>
         </children>
      </VBox>
      <ImageView fx:id="displayPicture" fitHeight="66.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
