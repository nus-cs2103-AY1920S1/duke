<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" style="-fx-background-color: #24daff; -fx-background-radius: 20;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" textAlignment="JUSTIFY" wrapText="true">
            <HBox.margin>
                <Insets bottom="1.0" left="5.0" right="5.0" top="1.0" />
            </HBox.margin>
            <padding>
                <Insets right="15.0" />
            </padding>
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin></ImageView>
    </children>
   <opaqueInsets>
      <Insets bottom="10.0" />
   </opaqueInsets>
</fx:root>
