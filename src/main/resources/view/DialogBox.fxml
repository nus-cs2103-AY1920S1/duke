<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
        <Label fx:id="dialog" text="Label">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
        <ImageView fx:id="displayPicture"  pickOnBounds="true" preserveRatio="false" />
   </children>
</fx:root>
