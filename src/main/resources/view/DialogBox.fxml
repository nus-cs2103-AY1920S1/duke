<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="380.0" prefHeight="177.0" prefWidth="380.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
