<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.HBox" id="dialog-box" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         minHeight="-Infinity" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <Label id="dialog-text" fx:id="dialog" text="Label" wrapText="true"/>
        <ImageView id="profile-picture" fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</fx:root>
