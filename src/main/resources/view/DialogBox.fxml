    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets top="4.0" />
         </HBox.margin></Label>
      <VBox>
         <children>
            <Circle fx:id="displayPictureCircle" fill="DODGERBLUE" radius="49.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="usernameLabel" alignment="CENTER" contentDisplay="CENTER" prefWidth="98.0" text="Label" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets left="6.0" right="6.0" />
         </padding>
      </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
