<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="messageBoxContainer" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <HBox fx:id="messageBox" style="-fx-background-color: #b2eee6; -fx-background-radius: 10;">
      <Label fx:id="message" lineSpacing="0.25" prefWidth="400" text="Label" wrapText="true">
        <font>
          <Font name="Consolas" size="14.0" />
        </font>
        <HBox.margin>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </HBox.margin>
      </Label>
    </HBox>
  </children>
</fx:root>
